{
	"name": "goboard",
	"version": "0.6.1",
	"description": "Render Go game diagrams from markdown code blocks",
	"main": "build/main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && cp plugin-dist/manifest.json .",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"update-baseline": "npx jest --config jest.baseline.config.js"
	},
	"keywords": [],
	"author": "Dmitry I. Sokolov",
	"license": "MIT",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.9.1",
		"@types/jest": "^30.0.0",
		"@types/jsdom": "^27.0.0",
		"@types/node": "^16.11.6",
		"@types/sharp": "^0.31.1",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.25.11",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"obsidian": "latest",
		"sharp": "^0.34.4",
		"ts-jest": "^29.4.4",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"jsdom": "^27.0.1"
	}
}
